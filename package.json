{
  "name": "io.eframework.unite.xhr",
  "displayName": "EFramework XHR for Unite",
  "description": "基于 PuerTS 的 XMLHttpRequest 标准实现，可运行于 Unity、Unreal 等多个引擎环境中。",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "EFramework Innovation",
    "email": "eframework-io@outlook.com",
    "url": "https://github.com/eframework-io"
  },
  "homepage": "https://eframework.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eframework-io/Unite.XHR.git"
  },
  "bugs": {
    "url": "https://github.com/eframework-io/Unite.XHR/issues"
  },
  "keywords": [
    "eframework",
    "unite",
    "xhr"
  ],
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "exports": {
    "./unity": {
      "require": "./dist/unity/index.cjs",
      "import": "./dist/unity/index.mjs",
      "types": "./dist/unity/index.d.ts"
    },
    "./unreal": {
      "require": "./dist/unreal/index.cjs",
      "import": "./dist/unreal/index.mjs",
      "types": "./dist/unreal/index.d.ts"
    },
    "./tests": {
      "require": "./dist/tests/index.cjs",
      "import": "./dist/tests/index.mjs",
      "types": "./dist/tests/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.7",
    "@types/url-parse": "1.4.11",
    "io.eframework.unite.utility": "0.1.0",
    "jest": "27.2.0",
    "rollup": "2.79.1",
    "rollup-plugin-dts": "4.2.3",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "27.0.0",
    "typescript": "4.1.6"
  },
  "scripts": {
    "debug": "rm -rf dist && rm -rf tests/dist && rollup -c && npm link",
    "release": "rm -rf dist && rm -rf tests/dist && rollup -c --mode=production",
    "test": "jest --coverage --runInBand",
    "prepublishOnly": "npm install && npm run release"
  },
  "dependencies": {
    "express": "4.21.2",
    "http-status-codes": "2.3.0"
  }
}